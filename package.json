{
  "name": "@openglobus/babylon",
  "version": "1.0.0",
  "description": "OpenGlobus rewritten with Babylon.js - A JavaScript library for interactive 3D maps and geospatial data visualization",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:tsc": "tsc",
    "build:watch": "tsc --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.mjs --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "vite"
  },
  "keywords": [
    "3d",
    "maps",
    "webgl",
    "babylon.js",
    "geospatial",
    "gis",
    "earth",
    "globe",
    "terrain",
    "visualization"
  ],
  "author": "OpenGlobus Team",
  "license": "MIT",
  "dependencies": {
    "@babylonjs/core": "^7.0.0",
    "@babylonjs/materials": "^7.0.0",
    "@babylonjs/loaders": "^7.0.0",
    "@babylonjs/post-processes": "^7.0.0",
    "@babylonjs/procedural-textures": "^7.0.0",
    "@babylonjs/serializers": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openglobus/openglobus.git"
  },
  "bugs": {
    "url": "https://github.com/openglobus/openglobus/issues"
  },
  "homepage": "https://openglobus.org"
}