{
  "name": "@openglobus/babylon",
  "version": "1.0.0",
  "description": "OpenGlobus rewritten with Babylon.js - A JavaScript library for interactive 3D maps and geospatial data visualization",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite --host --open",
    "start": "vite preview --host --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.mjs --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "3d",
    "maps",
    "webgl",
    "babylon.js",
    "geospatial",
    "gis",
    "earth",
    "globe",
    "terrain",
    "visualization"
  ],
  "author": "OpenGlobus Team",
  "license": "MIT",
  "dependencies": {
    "@babylonjs/core": "^8.25.0",
    "@babylonjs/loaders": "^8.25.0",
    "@babylonjs/materials": "^8.25.0",
    "@babylonjs/post-processes": "^8.25.0",
    "@babylonjs/procedural-textures": "^8.25.0",
    "@babylonjs/serializers": "^8.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openglobus/openglobus.git"
  },
  "bugs": {
    "url": "https://github.com/openglobus/openglobus/issues"
  },
  "homepage": "https://openglobus.org"
}
