{
  "name": "@openglobus/babylon",
  "version": "1.0.0",
  "type": "module",
  "description": "OpenGlobus rewritten with Babylon.js - A JavaScript library for interactive 3D maps and geospatial data visualization",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite --host --open",
    "start": "vite preview --host --open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.mjs --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "3d",
    "maps",
    "webgl",
    "babylon.js",
    "geospatial",
    "gis",
    "earth",
    "globe",
    "terrain",
    "visualization"
  ],
  "author": "OpenGlobus Team",
  "license": "MIT",
  "dependencies": {
    "@babylonjs/core": "^8.25.1",
    "@babylonjs/loaders": "^8.25.1",
    "@babylonjs/materials": "^8.25.1",
    "@babylonjs/post-processes": "^8.25.1",
    "@babylonjs/procedural-textures": "^8.25.1",
    "@babylonjs/serializers": "^8.25.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "ajv": "^8.17.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^4.5.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ]
}
